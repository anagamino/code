[ui]
username = Daniel Pa√±eda Rodriguez <dpaneda@tuenti.com>
merge = internal:merge
editor = vim

[extensions]
## hg view
hgext.hgk =
hgext.color =
mq =
inotify =
hgext.bookmarks =
hgext.graphlog =
transplant=
hggit = /home/daniel/share/hg-git/hggit
prompt = /home/daniel/share/hg-prompt/prompt.py

[inotify]
autostart = False

[color]
## Note: black bold looks gray! awesome :)
mode = auto
status.modified = magenta bold
status.added    = green   bold
status.removed  = red     bold
status.deleted  = cyan    bold
status.unknown  = black   bold
status.ignored  = black   bold
diff.trailingwhitespace = bold red_background

[paths]
stable = ssh://code.tuenti.com//srv/hg/tuenti/stable
release = ssh://code.tuenti.com//srv/hg/tuenti/release
integration = ssh://code.tuenti.com//srv/hg/tuenti/integration
hotfix = ssh://code.tuenti.com//srv/hg/tuenti/hotfix
configuration = ssh://dev02.tuenti.int//home/release/configuration/

[alias]
info = paths default

[bookmarks]
track.current = True
